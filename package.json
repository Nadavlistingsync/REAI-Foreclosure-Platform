{
  "name": "foreclosure-investment-platform",
  "version": "1.0.0",
  "description": "Comprehensive foreclosure investment and lead generation platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "seed": "node scripts/seedDatabase.js",
    "setup": "npm run install:all && npm run seed",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.3.6",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0",
    "stripe": "^13.5.0",
    "socket.io": "^4.7.2",
    "redis": "^4.6.7",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "sharp": "^0.32.5",
    "csv-parser": "^3.0.0",
    "pdf-parse": "^1.1.1",
    "geocoder": "^0.2.3",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "real-estate",
    "foreclosure",
    "investment",
    "lead-generation",
    "property-analysis"
  ],
  "author": "REAI Platform",
  "license": "MIT"
}
